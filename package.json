{
  "name": "sample-calculator-app",
  "version": "1.0.0",
  "description": "Sample application for SLIP 2 Q2 - Jenkins Pipeline with Unit Tests",
  "main": "src/calculator.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "echo 'Build completed successfully'",
    "start": "node src/app.js"
  },
  "keywords": [
    "calculator",
    "unit-testing",
    "jenkins",
    "devops"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}

